!Surface species
!==============================================================================
!Format is Label(S)/number of sites occupied/

SITE/SURFACE/      SDEN/2.49081E-9/
  
  NH3(S)/1/
  NH2(S)/1/
  NH(S)/1/
  N(S)/1/
  H(S)/1/
  N2(S)/1/
  PT(S)

BULK PT(B)/21.4/
END

!Reactions
!==============================================================================

REACTIONS MWOFF KCAL/MOLE
!*******************************************************************************
!Adsorption/Desorption
!*******************************************************************************

  H2 + 2PT(S) = 2H(S) + 2PT(B)                          0.5     0.00     0.00
  STICK
  N2 + PT(S)  = N2(S) + PT(B)                           0.5     0.00     0.00
  STICK
  NH3 + PT(S) = NH3(S) + PT(B)                          0.5     0.00     0.00
  STICK

!*******************************************************************************
!Dehydrogenation
!*******************************************************************************
  NH3(S) + PT(S) = NH2(S) + H(S) + PT(B)                    8.37E+18 1.00 00.00
  NH2(S) + PT(S) = NH(S)  + H(S) + PT(B)                    8.37E+18 1.00 00.00
  NH(S)  + PT(S) = N(S)   + H(S) + PT(B)                    8.37E+18 1.00 00.00  
!-------------------------------------------------------------------------------
!N2 association
!-------------------------------------------------------------------------------
  2N(S) + PT(B)  = N2(S)  +  PT(S)                          8.37E+18 1.00 00.00

END
